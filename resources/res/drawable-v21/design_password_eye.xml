<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true" android:id="@+id/visible" android:drawable="@drawable/design_ic_visibility"/>
    <item android:id="@g/masked" android:drawable="@f/design_ic_visibility_off"/>
    <transition android:drawable="@f/avd_show_password" android:toId="@+id/visible" android:fromId="@g/masked"/>
    <transition android:drawable="@f/avd_hide_password" android:toId="@g/masked" android:fromId="@+id/visible"/>
</animated-selector>
